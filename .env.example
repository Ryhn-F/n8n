# Render-like setup
N8N_PORT=5678
N8N_HOST=0.0.0.0
N8N_PROTOCOL=https
WEBHOOK_URL=https://<your-domain-or-ec2-ip>/

# Supabase Postgres
DB_TYPE=postgresdb
DB_POSTGRESDB_HOST=db.xxxxx.supabase.co
DB_POSTGRESDB_PORT=5432
DB_POSTGRESDB_DATABASE=postgres
DB_POSTGRESDB_USER=postgres
DB_POSTGRESDB_PASSWORD=<your-supabase-password>
DB_POSTGRESDB_SSL_REJECT_UNAUTHORIZED=false

# Security
N8N_ENCRYPTION_KEY=<long-random-secret>
